<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Test</title>
    <!-- Tell the browser to be responsive to screen width -->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- Font Awesome -->
    <link rel="stylesheet" href="/css/all.min.css">
    <!-- Ionicons -->
    <link rel="stylesheet" href="https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css">
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">
    <!-- Tempusdominus Bbootstrap 4 -->
    <link rel="stylesheet" href="/css/tempusdominus-bootstrap-4.min.css">
    <!-- iCheck -->
    <link rel="stylesheet" href="/css/icheck-bootstrap.min.css">
    <!-- JQVMap -->
    <link rel="stylesheet" href="/css/jqvmap.min.css">
    <!-- Theme style -->
    <link rel="stylesheet" href="/css/adminlte.min.css">
    <!-- overlayScrollbars -->
    <link rel="stylesheet" href="/css/OverlayScrollbars.min.css">
    <!-- Daterange picker -->
    <link rel="stylesheet" href="/css/daterangepicker.css">
    <!-- summernote -->
    <link rel="stylesheet" href="/css/summernote-bs4.css">
    <!-- Google Font: Source Sans Pro -->
    <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700" rel="stylesheet">
</head>


<body class="hold-transition sidebar-mini layout-fixed">
    <!-- Navbar -->
    <nav class="main-header navbar navbar-expand navbar-tran navbar-light bg-transparent">
        <!-- Left navbar links -->
        <ul class="navbar-nav">
            <li class="nav-item">
                <a class="nav-link" data-widget="pushmenu" href="#" role="button"><i class="fa fa-bars"></i></a>
            </li>
        </ul>
        
    </nav>
    <!-- /.navbar -->
    <!-- Main Sidebar Container -->
    <aside class="main-sidebar sidebar-dark-primary elevation-4">
        <!-- Brand Logo -->
        <a asp-action="Index" class="brand-link">
            <img src="~/images/ic02.jpeg" alt="logo"
                 style="height:60px; width:auto;">
            <span class="brand-text" style="font-weight:500; color:white; font-size:1.3em;">Yedi Test</span>
        </a><span><a href="https://github.com/KayaTS/ProjeTestSite" rel="nofollow">Proje</a></span>
        <!-- Sidebar -->
        <div class="sidebar">
            <!-- Sidebar user (optional) -->
            <div class="user-panel mt-3 pb-3 mb-3 d-flex">
                @*<div class="image">
                    <img src="/images/noface.png" class="img-circle elevation-2" alt="User Image">
                </div>*@
                <div class="info">
                    <a asp-action="Profil" class="d-block">
                        <i class="nav-icon fa fa-male text-blue"></i> Profilim
                    </a>
                    <!-- User.Claims.FirstOrDefault(f => f.Type == System.Security.Claims.ClaimTypes.Name).Value</a> -->
                </div>
            </div>

            <!-- Sidebar Menu -->
            <nav class="mt-2">
                <ul class="nav nav-pills nav-sidebar flex-column" data-widget="treeview" role="menu" data-accordion="false">
                    <!-- Add icons to the links using the .nav-icon class
                with font-awesome or any other icon font library -->


                    <li class="nav-item has-treeview">
                        <a href="#" class="nav-link">
                            <i class="nav-icon fa fa-gamepad text-purple"></i>
                            <p>
                                İçerik
                                <i class="fa fa-angle-right"></i>
                            </p>
                        </a>
                        <ul class="nav nav-treeview">
                            <li class="nav-item ml-lg-5">
                                <a asp-action="mekan" class="nav-link">
                                    <i class="fa fa-play nav-icon text-danger"></i>
                                    <p>Mekanlar</p>
                                </a>
                            </li>
                            <li class="nav-item ml-lg-5">
                                <a asp-action="unity" class="nav-link">
                                    <i class="fa fa-user-plus nav-icon text-blue"></i>
                                    <p>Oyun</p>
                                </a>
                            </li>
                            <li class="nav-item ml-lg-5">
                                <a asp-action="harita" class="nav-link">
                                    <i class="fa fa-map nav-icon text-white"></i>
                                    <p>Harita</p>
                                </a>
                            </li>
                            <li class="nav-item ml-lg-5">
                                <a asp-action="Yapiliyor" class="nav-link">
                                    <i class="fa fa-plus-circle nav-icon "></i>
                                    <p>Fotoğraf Ekle</p>
                                </a>
                            </li>
                        </ul>
                    </li>
                    <li class="nav-item">
                        <a asp-action="istangram" class="nav-link">
                            <i class="nav-icon fa fa-thumbs-up text-red" aria-hidden="true"></i>
                            <p>
                                İstangram
                            </p>
                        </a>
                    </li>
                    <li class="nav-item">
                        <a asp-action="Yapiliyor" class="nav-link">
                            <i class="nav-icon fa fa-binoculars text-white" aria-hidden="true"></i>
                            <p>
                                Rehber
                            </p>
                        </a>
                    </li>
                    <li class="nav-item">
                        <a asp-action="Yapiliyor" class="nav-link">
                            <i class="nav-icon fa fa-star text-cyan" aria-hidden="true"></i>
                            <p>
                                Başarımlar
                            </p>
                        </a>
                    </li>
                    <li class="nav-item">
                        <a asp-action="Yapiliyor" class="nav-link">
                            <i class="nav-icon fa fa-trophy text-blue" aria-hidden="true"></i>
                            <p>
                                Ödüller
                            </p>
                        </a>
                    </li>

                    <li class="nav-item">
                        <a asp-action="Sohbet" class="nav-link">
                            <i class="fa fa-comments nav-icon text-green"></i>
                            <p>
                                Mesajlar
                            </p>
                        </a>
                    </li>
                    <li class="nav-item">
                        <a asp-action="Yapiliyor" class="nav-link">
                            <i class="nav-icon fa fa-question text-yellow"></i>
                            <p>
                                Öneri bildir
                            </p>
                        </a>
                    </li>

                    <li class="nav-item mt-auto">
                        <a asp-action="Yapiliyor" class="nav-link">
                            <i class="nav-icon fa fa-cog text-gray"></i>
                            <p>Ayarlar</p>
                        </a>
                    </li>
                    <li class="nav-item" style="margin-top:auto;">
                        <a asp-area="uye" asp-controller="Home" asp-action="Cikis" class="nav-link">
                            <i class="nav-icon fa fa-power-off text-black"></i>
                            <p>Çıkış Yap</p>
                        </a>
                    </li>
                </ul>
            </nav>
            <!-- /.sidebar-menu -->
        </div>
        <!-- /.sidebar -->
    </aside>

    @RenderBody()

    <footer class="main-footer bg-info">
        <div class="float-right d-none d-sm-inline-block ">
            <b></b>
        </div>
    </footer>


    <script src="~/jquery/jquery.min.js"></script>
    <!-- jQuery UI 1.11.4 -->
    <script src="~/jquery/jquery-ui.min.js"></script>
    <!-- Resolve conflict in jQuery UI tooltip with Bootstrap tooltip -->
    <script>$.widget.bridge('uibutton', $.ui.button)</script>
    <!-- Bootstrap 4 -->
    <script src="/js/bootstrap.bundle.min.js"></script>
    <!-- ChartJS -->
    <script src="/js/Chart.min.js"></script>
    <!-- Sparkline -->
    <script src="/js/sparkline.js"></script>
    <!-- JQVMap -->
    <script src="/js/jquery.vmap.min.js"></script>
    <script src="/js/jquery.vmap.turkey.js"></script>
    <!-- jQuery Knob Chart -->
    <script src="/js/jquery.knob.min.js"></script>
    <!-- daterangepicker -->
    <script src="/js/moment.min.js"></script>
    <script src="/js/daterangepicker.js"></script>
    <!-- Tempusdominus Bootstrap 4 -->
    <script src="/js/tempusdominus-bootstrap-4.min.js"></script>
    <!-- Summernote -->
    <script src="/js/summernote-bs4.js"></script>
    <!-- overlayScrollbars -->
    <script src="/js/jquery.overlayScrollbars.min.js"></script>
    <!-- AdminLTE App -->
    <script src="/js/adminlte.js"></script>
    <!-- AdminLTE dashboard demo (This is only for demo purposes) -->
    <script src="/js/dashboard.js"></script>
    <!-- AdminLTE for demo purposes -->
    <script src="/js/demo.js"></script>
</body>

</html>